spring:
  cloud:
    nacos:
      discovery: 
        ip: ${spring.application.name}
        port: 50005
  ####################################################################################
 ####################################################################################
  ###################################DataSource Config################################
  ####################################################################################
  datasource:
    # JDBC 配置
    type: com.alibaba.druid.pool.DruidDataSource
    driver-class-name: com.mysql.cj.jdbc.Driver
    url: jdbc:mysql://{{ EDOPS_MYSQL_HOST }}:{{ EDOPS_MYSQL_PORT }}/ly_ac_admin_db?useUnicode=true&characterEncoding=utf8&serverTimezone=Asia/Shanghai&useSSL=false
    username: {{ EDOPS_MYSQL_ROOT_USER }}
    password: {{ EDOPS_MYSQL_ROOT_PASSWORD }}
    
 ########################## 当前租户RSA配置 ##################################
tenant:
  public: {{ EDOPS_TENANT_PUBLIC_MODE }}
  school-id: {{ EDOPS_TENANT_SCHOOL_ID }}
  enable: true
  secure-key: UIuqNdAyioWvFdfr
  # 默认指向正式环境域名，https://restapi.118class.com:800
  connect-prefix-url: {{ EDOPS_API_TENANT_URL }}
  system:
    public-key: 
  local:
    public-key: 
    private-key: 

########################## LY配置 ##################################
ly:
  tenant:
    tenant-mode: true
    exclude-tables:
      - ly_jx_xtgl_xtcsb
      - ly_jx_xtgl_dlpzb

